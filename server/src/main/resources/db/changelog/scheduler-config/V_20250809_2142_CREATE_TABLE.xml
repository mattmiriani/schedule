<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.8.xsd">

    <changeSet id="create-table" author="matt (generated)">
        <preConditions>
            <not>
                <tableExists tableName="schedule_config"/>
            </not>
        </preConditions>

        <createTable tableName="schedule_config">
            <column name="id" type="${uuid}" defaultValueComputed="${uuid_default}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="${datahorazone}"/>
            <column name="updated_at" type="${datahorazone}"/>

            <column name="start_date" type="${data}"/>
            <column name="finish_date" type="${data}"/>
            <column name="start_time" type="${hora}"/>
            <column name="finish_time" type="${hora}"/>
            <column name="duration" type="${hora}"/>
            <column name="day_of_week" type="${descricaoenum}"/>
        </createTable>
    </changeSet>
</databaseChangeLog>